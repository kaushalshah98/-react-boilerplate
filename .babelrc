{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript",
    "@emotion/babel-preset-css-prop"
  ],
  "plugins": [
    ["@emotion"],
    [
      "babel-plugin-styled-components",
      {
        "pure": true
        // "fileName": false,
        // "displayName": false,
        // "ssr": false
      }
    ],
    ["@styles-system/babel-plugin"],
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}
